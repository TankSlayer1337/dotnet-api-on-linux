{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "Kestrel": {
    "Endpoints": {
      "Http": {
        "Url": "http://*:80"
      },
      "Https": {
        "Url": "https://*:443",
        "Certificate": {
          "Path": "/etc/letsencrypt/live/weather.cloudchaotic.com/fullchain.pem",
          "KeyPath": "/etc/letsencrypt/live/weather.cloudchaotic.com/privkey.pem"
        }
      }
    }
  }
}
